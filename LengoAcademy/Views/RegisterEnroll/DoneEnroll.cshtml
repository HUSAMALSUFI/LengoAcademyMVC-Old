@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Head{
 <script>
        const myTimeout = setTimeout(myGreeting, 3000);
        function myGreeting() {
        window.location.href = '@Url.Action("Index","Home")';
        }
    </script>}
@section Body{
    <main id="main-body-wrapper">
        <!-- Error Section -->
        <section class="maan-error-section">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="maan-error">
                            <div class="maan-error-img">
                                <img src="~/assets/img/imgDone.png" alt="" style="margin-left: 358px;margin-bottom: 56px; margin-top: 56px;">
                            </div>
                           <div class="maan-error-content">
                                <h2>Thank you</h2>
                                <p>for filling the Registeration form, we will contact you soon.</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- Error Section -->
        <!-- CTA Two Section -->
        <section class="maan-cta-two-area">
            <div class="container">
                <div class="row">
                    <div class="col-12" data-cue="slideInUp">
                        <div class="maan-cta-wrapper" data-background="assets/img/bg/cta-bg2.svg">
                            <div class="cta-content">
                                <h2>Are you interested in sharing your course?</h2>
                                <a asp-area="" asp-controller="RegisterEnroll" asp-action="Enroll" class="btn btn-red btn-lg">Join Now</a>
                            </div>
                            <img src="~/assets/img/svg/star-1.svg" alt="SVG" class="shape">
                            <img src="~/assets/img/svg/star-2.svg" alt="SVG" class="shape2">
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- CTA Two Section -->
    </main>
}
